<%= render partial: "shared/nav_bar", locals: {title: "Create new group", title_amount: nil, back_button_path: group_index_path} %>
<%= form_with model: @group, url: "/group", method: :post, scope: :group do |form| %>
  <%= form.text_field :name %>
  <%= form.text_field :icon %>
  <%= form.submit "Save", class: 'new-btn' %>
<% end %>
